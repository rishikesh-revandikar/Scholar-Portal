                                        <option value="icwai">ICWAI </option>
                                        <option value="gnm">GNM </option>
                                        <option value="anm">ANM </option>
                                        <option value="not-mentioned">Not Mentioned </option>
                                        <option value="bmlt">BMLT </option>
                                        <option value="dmlt">DMLT </option>
                                    </select>
                                </li>
                                <li>
                                    <select class="form-control basic-select" id="category" name="category">
                                        <option value="">Category </option>
                                        <option value="general">General </option>
                                        <option value="sc_st_obc">SC/ST/OBC </option>
                                        <option value="girls">Girls </option>
                                        <option value="minority">Minority </option>
                                        <option value="physically_challenged">Physically Challenged </option>
                                        <option value="economically_backward_class">Economically Backward Class
                                        </option>
                                        <option value="for_all">For All </option>
                                        <option value="obc">BPL </option>
                                        <option value="bpl-below-poverty-line">BPL-Below Poverty Line </option>
                                    </select>
                                </li>
                                <li>
                                    <select class="form-control basic-select" id="origin" name="origin">
                                        <option value="">Origin</option>
                                        <option value="national">National </option>
                                        <option value="international">International </option>
                                    </select>
                                </li>


                                <li>
                                    <select class="form-control basic-select" id="type" name="type">
                                        <option value="">Type</option>
                                        <option value="scholarship">Scholarship </option>
                                        <option value="fellowship">Fellowship </option>
                                    </select>
                                </li>

                                <li>
                                    <select class="form-control basic-select" id="availability" name="availability">
                                        <option value="" selected>Availability </option>
                                        <option value="open">Active </option>
                                        <option value="close">Inactive </option>

                                    </select>
                                </li>
                                <div class="form-group button">
                                    <input name="form_botcheck" class="form-control" type="hidden" value="">
                                    <button type="submit" class="btn col-lg-12 cus-bt"
                                        data-loading-text="Please wait...">Apply </button>
                                    <input type="button" class="btn btn-outline-primary col-lg-12 pad-10 cus-bt"
                                        style="background-color: #8d529a; color: #fff;"
                                        data-loading-text="Please wait..." onclick="Reset();" value="Clear">
                                </div>

                            </ul>
                        </form>
                    </div>
                </div>
            </div>

     <%       for (let i = 0; i < <%=  %> ; i++) {     %>
